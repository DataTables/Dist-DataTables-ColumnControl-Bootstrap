<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <version>1.0.3</version>
        <id>datatables.net-columncontrol-bs</id>
        <description>This is ColumnControl for DataTables with styling for [Bootstrap](https://getbootstrap.com/docs/3.3/)</description>
        <repository type="git" url="https://github.com/DataTables/Dist-DataTables-ColumnControl-Bootstrap.git"/>
        <authors>SpryMedia Ltd</authors>
        <projectUrl>http://datatables.net</projectUrl>
        <license type="expression">MIT</license>
        <tags>ColumnControl  search  filter  buttons  column visibility  Bootstrap  Datatables jQuery table</tags>
        <dependencies>
            <dependency id="datatables.net-columncontrol" version="0.0.1"/>
            <dependency id="datatables.net-bs" version="2.3.1"/>
        </dependencies>
        <contentFiles>
            <files include="**/*" buildAction="Content"/>
        </contentFiles>
    </metadata>
    <files>
        <file src="js/columnControl.bootstrap.js" target="content\Scripts"/>
        <file src="js/columnControl.bootstrap.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="js/columnControl.bootstrap.min.js" target="content\Scripts"/>
        <file src="js/columnControl.bootstrap.min.js" target="contentFiles\any\any\wwwroot\js"/>
        <file src="css/columnControl.bootstrap.css" target="content\Content"/>
        <file src="css/columnControl.bootstrap.css" target="contentFiles\any\any\wwwroot\css"/>
        <file src="css/columnControl.bootstrap.min.css" target="content\Content"/>
        <file src="css/columnControl.bootstrap.min.css" target="contentFiles\any\any\wwwroot\css"/>
    </files>
</package>

